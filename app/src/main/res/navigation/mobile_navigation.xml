<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.klim.architecture.ui.generator.HomeFragment"
        android:label="@string/title_generate"
        tools:layout="@layout/fragment_generator" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.klim.architecture.ui.saved.DashboardFragment"
        android:label="@string/title_saved"
        tools:layout="@layout/fragment_saved" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.klim.architecture.ui.settings.SettingsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />
</navigation>