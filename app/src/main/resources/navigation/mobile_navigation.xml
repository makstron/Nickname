<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_generator">

    <fragment
        android:id="@+id/navigation_generator"
        android:name="com.klim.nickname.app.window.generator.GeneratorFragment"
        android:label="@string/title_generate"
        tools:layout="@layout/fragment_generator" />

    <fragment
        android:id="@+id/navigation_store"
        android:name="com.klim.nickname.app.window.saved.StoreFragment"
        android:label="@string/title_saved"
        tools:layout="@layout/fragment_store" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.klim.nickname.app.window.settings.SettingsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />
</navigation>